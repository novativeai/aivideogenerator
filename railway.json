  },
  "deploy": {
    "startCommand": "echo \"$GCP_SERVICE_ACCOUNT_JSON\" > /tmp/gcp-creds.json && export GOOGLE_APPLICATION_CREDENTIALS=/tmp/gcp-creds.json && hypercorn main:app --bind \"[::]:$PORT\""
  }
}